  "scripts": {
    "ng": "ng",
    "lint": "tslint \"src/**/*.ts\"",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "pre-dist": "rm -rf ../assets; rm -f ../favicon.ico; rm -f ../index.html; rm -f ../*.bundle.*; rm -f ../sw-precache-config.js",
